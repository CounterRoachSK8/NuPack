<?xml version="1.0" encoding="utf-8"?>
<Project ToolsVersion="4.0" xmlns="http://schemas.microsoft.com/developer/msbuild/2003">
    <PropertyGroup>
        <BuildDependsOn>
            $(BuildDependsOn);
            [name]
        </BuildDependsOn>
    </PropertyGroup>
    <Target Name="[name]">
        <Exec Command="&quot;$(SolutionDir)packages\[name].[version]\build\[name].exe&quot; &quot;$(SolutionDir)\&quot; &quot;$(ProjectPath)&quot; &quot;$(Configuration)&quot;" /> 
    </Target>
</Project>